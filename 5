using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int a, b, c, i;
            Console.Write("Введите число а: ");
            a = int.Parse(Console.ReadLine());
            Console.Write("Введите число b: ");
            b = int.Parse(Console.ReadLine());
            c = a % b;
            do
            {
                c = a % b;
                a = b;
                b = c;
            }
            while (c != 0);
            Console.Write("Наибольший общий делитель для этих чисел равен: ");
            Console.WriteLine(a);
            Console.ReadKey();
        }
    }
}
