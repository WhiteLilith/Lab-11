using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int a, b, N, i, c, p;
            Console.Write("Введите N: ");
            N = int.Parse(Console.ReadLine());
            a = 1;
            b = 1;
            c = 0;
            p = 1;
            for (i = 1; i <= N; i++)
            {
                if (c <= N)
                {
                    c = a + b;
                    a = b;
                    b = c;
                    p = p + 1;
                }
            }
            if (N == 1)
            {
                Console.WriteLine("Это число находится в ряду Фибоначчи под номером 1 и 2");
            }
            else
            {
                Console.Write("Это число находится в рядy Фибоначчи под номером ");
                Console.WriteLine(p);
            }
            Console.ReadKey();
        }
    }
}
